{
  "name": "http.8002",
  "virtualHosts": [
    {
      "name": "dashboard.sandbox25.gygkube.com:8002",
      "domains": [
        "dashboard.sandbox25.gygkube.com",
        "dashboard.sandbox25.gygkube.com:*"
      ],
      "routes": [
        {
          "match": {
            "prefix": "/"
          },
          "route": {
            "cluster": "outbound|80||kubernetes-dashboard.kubernetes-dashboard.svc.cluster.local",
            "timeout": "0s",
            "retryPolicy": {
              "retryOn": "connect-failure,refused-stream,unavailable,cancelled,retriable-status-codes",
              "numRetries": 2,
              "hostSelectionRetryMaxAttempts": "5",
              "retriableStatusCodes": [
                503
              ]
            },
            "maxGrpcTimeout": "0s"
          },
          "metadata": {
            "filterMetadata": {
              "istio": {
                "config": "/apis/networking.istio.io/v1alpha3/namespaces/kubernetes-dashboard/virtual-service/kubernetes-dashboard"
              }
            }
          },
          "decorator": {
            "operation": "kubernetes-dashboard.kubernetes-dashboard.svc.cluster.local:80/*"
          }
        }
      ],
      "includeRequestAttemptCount": true
    },
    {
      "name": "httpbin.sandbox25.gygkube.com:8002",
      "domains": [
        "httpbin.sandbox25.gygkube.com",
        "httpbin.sandbox25.gygkube.com:*"
      ],
      "routes": [
        {
          "match": {
            "path": "/headers",
            "caseSensitive": true
          },
          "route": {
            "cluster": "outbound|80||httpbin.httpbin.svc.cluster.local",
            "timeout": "0s",
            "retryPolicy": {
              "retryOn": "connect-failure,refused-stream,unavailable,cancelled,retriable-status-codes",
              "numRetries": 2,
              "hostSelectionRetryMaxAttempts": "5",
              "retriableStatusCodes": [
                503
              ]
            },
            "maxGrpcTimeout": "0s"
          },
          "metadata": {
            "filterMetadata": {
              "istio": {
                "config": "/apis/networking.istio.io/v1alpha3/namespaces/httpbin/virtual-service/httpbin"
              }
            }
          },
          "decorator": {
            "operation": "httpbin.httpbin.svc.cluster.local:80/headers"
          }
        }
      ],
      "includeRequestAttemptCount": true
    }
  ],
  "validateClusters": false
}